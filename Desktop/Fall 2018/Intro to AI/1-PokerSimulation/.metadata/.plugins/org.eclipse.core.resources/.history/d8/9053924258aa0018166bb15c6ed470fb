package PokerSimulation.main;

public class Deck {
	private Card deck[] = new Card[52];
	
	public Deck() 
	{
		int card_num = 0;
		
		for (int i = 0; i<4; i++) {
			for (int j=0; j<13; j++) {
				Card temp_card = new Card(Suites[i], Values[j]);
				deck[card_num] = temp_card;
				card_num++;
			}
		}
	}
	
	

}
